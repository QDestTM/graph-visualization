config_version=5

[application]

config/name="Graph Visualisation"
config/version="1.1.1"
config/tags=PackedStringArray("2d", "not_game")
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
run/delta_smoothing=false
boot_splash/show_image=false
config/icon="res://resources/icons/app/icon_512.png"
config/windows_native_icon="res://resources/icons/app/icon.ico"

[autoload]

Enums="*res://scripts/autoload/Enums.gd"
Globals="*res://scripts/autoload/Globals.gd"
GraphManager="*res://scripts/autoload/GraphManager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/subwindows/embed_subwindows=false

[dotnet]

project/assembly_name="GraphVisualisation"

[editor]

movie_writer/mjpeg_quality=1.0
movie_writer/movie_file="E:/GodotEngine/graph-visualization/preview.avi"
naming/node_name_num_separator=3
naming/scene_name_casing=1

